l=[]
n=int(input("Enter the number of elements:"))
for i in range(n):
    b=int(input("Enter the  element:"))
    l.append(b)
def sumsquare(l):
   odd=0
   even=0
   for i in l:
       if i%2==0:
          even = even + i**2
       else:
          odd = odd + i**2
   l=[odd,even]
   return(l)
print(sumsquare(l))
